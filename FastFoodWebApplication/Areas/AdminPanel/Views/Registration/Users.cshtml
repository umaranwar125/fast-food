@model FastFoodWebApplication.Models.RegisterModel

<!-- Breadcrumb -->
<h2 class="heading">Registration</h2>
<ol class="breadcrumb">
    <li>@Html.ActionLink("Dashboard", "Statistics", new { @area = "AdminPanel", Controller = "Registration" })</li>
    <li>@Html.ActionLink("Registration", "Users", new { @area = "AdminPanel", Controller = "Registration" })</li>
    <li class="active">Data</li>
</ol>
<!-- Breadcrumb -->

<div class="registered-users">
    <div class="bg">
        <div class="clearfix"></div>
        <div class="table-responsive">
            <p class="text-center text-danger">@TempData["DeleteSuccess"]</p>
            @if (ViewBag.TotalList != 0)
            {
            <table class="table table-responsive table-bordered table-hover ">
                <tr>
                    <th>Delete</th>
                    <th>Register ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email Address</th>
                    <th>Password</th>
                </tr>
                @foreach (var data in ViewBag.List)
                {
                <tr>
                    <td>@Html.ActionLink("Delete", "Delete", new { @area = "AdminPanel", Controller = "Registration", @Deleteid = data.RegisterID }, null)</td>
                    <td>@data.RegisterID</td>
                    <td>@data.FirstName</td>
                    <td>@data.LastName</td>
                    <td>@data.EmailAddress</td>
                    <td>@data.Password</td>
                </tr>
                }
            </table>
            }
            else
            {
                <p class="text-center" style="font-size: 18px; letter-spacing: 1px">Sorry! No data found.</p>
            }
            <!-- Table -->
        </div>
        <!-- Table Responsive -->
    </div>
    <!-- BG -->
</div>
<!-- Registetred User -->
